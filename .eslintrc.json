{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_|this",
        "varsIgnorePattern": "^_|^[A-Z0-9_]+$",
        "ignoreRestSiblings": true,
        "vars": "all",
        "args": "after-used",
        "caughtErrors": "none"
      }
    ]
  }
}
